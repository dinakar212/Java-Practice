import java.util.Scanner;
class Leap
{
public static void main(String []x)
{
 int a;
System.out.println("ENTER THE YEAR");
Scanner in = new Scanner(System.in);
a=in.nextInt();
if(a%4==0)
System.out.println("LEAP YEAR");
elseif(a%400==0)
System.out.println("LEAP YEAR");
else
System.out.println("NOT A LEAP YEAR");
}
}
